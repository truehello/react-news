    "date-fns": "^1.30.1",
    "netlify-lambda": "^1.6.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hooks-async": "^3.10.1",
    "react-image": "^2.1.1",
    "react-key-index": "^0.1.1",
